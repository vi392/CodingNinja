import java.util.Scanner;

public class SquareRoot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     Scanner s= new Scanner(System.in);
     int n=s.nextInt();
     
//     int sq_root=n/2;
//     System.out.println(sq_root);
     
        int i=1;
        while(i*i<=n) {
        	i++;
        	
        }
        System.out.println(i-1);
	}

}
